@-
@
text
@// $Id: printnums.c,v 1.1 2013-04-22 14:00:43-07 - - $

// Illustrate %.15g format.

#include <stdio.h>

int main (int argc, char **argv) {
   for (;;) {
      double number;
      int rc = scanf ("%lg", &number);
      if (rc != 1) break;
      printf ("%.15g\n", number);
   }
   return 0;
}
